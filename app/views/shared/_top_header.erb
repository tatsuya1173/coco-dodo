<header class='top-page-header'>
  <div class='search-bar-contents'>
    <%= link_to image_tag("cook-do1.png", class:"cook-do-icon", id:"cook-do1"), "/" %>
    <form class="search-form" action="#">
      <input class='input-box' placeholder='キーワードから探す'>
      <button class="search-button">
        <%= image_tag "search.png", class:"search-icon", id:"search-icon" %>
      </button>
    </form>
  </div>
  <div class='nav'>
    <ul class='lists-left'>
      <li><%= link_to 'ジャンル', "#", class: "category-list" %></li>
    </ul>
    <ul class='lists-right'>
       <% if user_signed_in? %>
       <li><%= link_to 'レシピ作成', '/recipes/new', method: :get, class: "recipe_new" %></li>
       <li><%= link_to current_user.nickname, "", class: "user-nickname" %></li>
       <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: "logout" %></li>
       <% else %>
       <li><%= link_to 'ログイン', new_user_session_path, method: :get, class: "login" %></li>
       <li><%= link_to '新規登録', new_user_registration_path, method: :get, class: "sign-up" %></li>
       <% end %>
    </ul>
  </div>
</header>

 

